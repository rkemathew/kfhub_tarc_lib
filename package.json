{
  "name": "kfhub_tarc_lib",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:dev": "concurrently --kill-others \"ng serve\" \"npm run sync:assets\" \"npm run sync:languages\" \"npm run sync:less\"",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "sync": "sync-files ../kfhub_app/src/assets ./src/assets && sync-files ../kfhub_app/src/languages ./src/languages && sync-files ../kfhub_app/src/app/modules/shared/_kfcore-less ./src/app/modules/shared/_kfcore-less",
    "sync:assets": "sync-files --watch ../kfhub_app/src/assets ./src/assets",
    "sync:languages": "sync-files --watch ../kfhub_app/src/languages ./src/languages",
    "sync:less": "sync-files --watch ../kfhub_app/src/app/modules/shared/_kfcore-less ./src/app/modules/shared/_kfcore-less",
    "rebuild:lib": "bash scripts/rebuild_kfhub_lib.sh",
    "rebuild:module": "bash scripts/rebuild_kfhub_module.sh"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/common": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/forms": "^5.2.3",
    "@angular/http": "^5.2.3",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@angular/router": "^5.2.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "kfhub_lib": "../kfhub_lib/dist/kfhub_lib-0.0.0.tgz",
    "ng-http-loader": "^0.7.0",
    "ngx-modialog": "^5.0.0",
    "primeng": "^5.2.0",
    "primeng-advanced-growl": "^3.1.3",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/language-service": "^5.2.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.0.0",
    "protractor": "~5.1.2",
    "sync-files": "^1.0.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.6.0"
  }
}
